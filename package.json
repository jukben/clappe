    "dev:chrome": "NODE_ENV=development gulp --watch --vendor=chrome",
    "dev:firefox": "NODE_ENV=development gulp --watch --vendor=firefox",
    "dev:opera": "NODE_ENV=development gulp --watch --vendor=opera",
    "dev:edge": "NODE_ENV=development gulp --watch --vendor=edge",
